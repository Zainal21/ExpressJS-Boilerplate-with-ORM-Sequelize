{
  "name": "expressjs-boilerplate",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "serve": "node ./bin/www",
    "test": "mocha",
    "dev": "nodemon ./bin/www",
    "db:init": "npx sequelize-cli init --force",
    "db:reset": "npx sequelize-cli db:drop && npx sequelize-cli db:create && npx sequelize-cli db:migrate && npx sequelize-cli db:seed:all",
    "prestart": "./node_modules/.bin/sequelize db:migrate && ./node_modules/.bin/sequelize db:seed:all"
  },
  "dependencies": {
    "@types/jsonwebtoken": "^8.5.6",
    "Base64": "^1.1.0",
    "bcryptjs": "^2.4.3",
    "cookie-parser": "~1.4.4",
    "cors": "^2.8.5",
    "debug": "~2.6.9",
    "dotenv": "^8.2.0",
    "ejs": "~2.6.1",
    "email-templates": "^8.0.8",
    "express": "~4.16.1",
    "faker": "^5.5.3",
    "fastest-validator": "^1.12.0",
    "global": "^4.4.0",
    "http-errors": "~1.6.3",
    "jsonwebtoken": "^8.5.1",
    "morgan": "^1.10.0",
    "multer": "^1.4.4",
    "mysql2": "^2.2.5",
    "nodemon": "^2.0.20",
    "pg": "^8.7.1",
    "pm2": "^5.1.2",
    "sequelize": "^6.3.5",
    "slug": "^5.1.1"
  },
  "devDependencies": {
    "mocha": "^8.1.3",
    "sequelize-cli": "^6.2.0"
  }
}
